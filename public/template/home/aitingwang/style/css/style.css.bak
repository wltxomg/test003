/*tab*/
.cd-tabs{position:relative;}
.cd-tabs nav{overflow:auto;-webkit-overflow-scrolling:touch;)}
.cd-tabs-navigation li{float:left}
.cd-tabs-navigation a{position:relative;display:block;-webkit-font-smoothing:antialiased;-moz-osx-font-smoothing:grayscale;}

.cd-tabs-content .cd-tabs-li{display:none;}
.cd-tabs-content li.selected{display:block;-webkit-animation:cd-fade-in .5s;-moz-animation:cd-fade-in .5s;animation:cd-fade-in .5s}
@-webkit-keyframes cd-fade-in{0%{opacity:0}
100%{opacity:1}
}
@-moz-keyframes cd-fade-in{0%{opacity:0}
100%{opacity:1}
}
@keyframes cd-fade-in{0%{opacity:0}
100%{opacity:1}
}


